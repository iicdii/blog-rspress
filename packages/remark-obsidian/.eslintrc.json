{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
    "quotes": "off",
    "operator-linebreak": "off",
    "implicit-arrow-linebreak": "off",
    "no-else-return": "off",
    "consistent-return": "off",
    "no-param-reassign": "off",
    "max-len": [0],
    "object-curly-newline": [0],
    "import/no-extraneous-dependencies": [0],
    "no-restricted-syntax": [0],
    "complexity": "off",
    "max-depth": [2, 2]
  }
}
